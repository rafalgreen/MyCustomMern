{
  "name": "mycustommerndemo",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node app.js",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon app.js",
    "client": "npm start --prefix client",
    "client-install": "cd client && npm install",
    "client-build": "cd client && npm run build"
  },
  "dependencies": {
    "concurrently": "^3.5.1",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.9",
    "ejs": "~2.5.7",
    "express": "~4.16.0",
    "http-errors": "~1.6.2",
    "install": "^0.11.0",
    "mongoose": "^5.0.14",
    "morgan": "~1.9.0"
  },
  "devDependencies": {
    "nodemon": "^1.17.3"
  }
}
